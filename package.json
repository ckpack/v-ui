{
  "name": "@ckpack/v-ui",
  "version": "1.3.0",
  "description": "Quickly create a component library for Vue 3",
  "sideEffects": false,
  "type": "module",
  "main": "es/index.js",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "packageManager": "pnpm@7.8.0",
  "exports": {
    ".": {
      "import": "./es/index.js",
      "require": "./dist/index.cjs.js"
    },
    "./dist/*": "./dist/*",
    "./es": "./es/index.js",
    "./es/*": "./es/*",
    "./src": "./src/index.ts",
    "./src/*": "./src/*"
  },
  "repository": "https://github.com/ckpack/v-ui",
  "homepage": "https://ckpack.github.io/v-ui",
  "author": "ckvv",
  "license": "ISC",
  "keywords": [
    "vue",
    "v-ui",
    "vue-ui"
  ],
  "files": [
    "src/*",
    "es/*",
    "dist/*"
  ],
  "scripts": {
    "prepare": "husky install",
    "predev": "pnpm link ./",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "release": "standard-version",
    "dev": "vite -c ./dev/vite.config.ts",
    "build": "npm run build:es && npm run build:dist",
    "build:es": "vite build  -c scripts/vite.es.ts",
    "build:dist": "vite build -c scripts/vite.dist.ts",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "gen": "vite-node ./scripts/gen.ts",
    "postgen": "vite-node ./scripts/gen-compoents.ts"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.25.2",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.21.0",
    "fast-glob": "^3.2.11",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "sass": "^1.54.3",
    "standard-version": "^9.5.0",
    "typescript": "^4.7.4",
    "unplugin-vue-define-options": "^0.7.3",
    "vite": "^3.0.4",
    "vite-node": "^0.20.3",
    "vite-plugin-dts": "^1.4.1",
    "vitepress": "^1.0.0-alpha.4",
    "vue": "^3.2.37"
  },
  "peerDependencies": {
    "vue": "^3.2.x"
  },
  "engines": {
    "node": ">=16"
  },
  "lint-staged": {
    "*.{js,ts,mjs,cjs,tsx,jsx,vue}": [
      "eslint --fix"
    ]
  }
}
